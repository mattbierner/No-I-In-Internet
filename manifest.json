{
    "manifest_version": 2,

    "name": "i-i-I-i-i",
    "description": "",
    "version": "1.0",
    
    "browser_action": {
        "name": "Manipulate DOM",
        "icons": ["icon.png"],
        "default_icon": "icon.png"
    },
    "permissions": ["activeTab"],
    "content_scripts": [{
        "matches": [ "http://*/*", "https://*/*"],
        "js": [
            "jquery.min.js",
            "i-i-I-i-i.js",
            "background.js" ],
        "css": [
            "background.css"],
        "run_at": "document_end"
    }]
}